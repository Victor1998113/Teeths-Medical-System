<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oral.mapper.MemberMapper">

    <resultMap id="BaseResultMap" type="com.oral.bean.Member">
            <id property="memberid" column="memberid" jdbcType="INTEGER"/>
            <result property="mname" column="mname" jdbcType="VARCHAR"/>
            <result property="mage" column="mage" jdbcType="INTEGER"/>
            <result property="mgender" column="mgender" jdbcType="VARCHAR"/>
            <result property="mphone" column="mphone" jdbcType="VARCHAR"/>
            <result property="mdate" column="mdate" jdbcType="TIMESTAMP"/>
            <result property="mmoney" column="mmoney" jdbcType="DECIMAL"/>
    </resultMap>

    <sql id="Base_Column_List">
        memberid,mname,mage,
        mgender,mphone,mdate,
        mmoney
    </sql>
</mapper>
